{
    "name": "enjoys/server-request-wrapper",
    "description": "Wrapper ServerRequestInterface PSR-7",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Enjoys\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Enjoys",
            "email": "deadly.info@gmail.com"
        }
    ],
    "require": {
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/psr7": "^2.2",
        "vimeo/psalm": "^4.7",
        "infection/infection": "0.26.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "scripts": {
        "phpunit": "./vendor/bin/phpunit",
        "psalm": "./vendor/bin/psalm",
        "psalm-ext": "./vendor/bin/psalm --show-info=true",
        "infection": "XDEBUG_MODE=coverage ./vendor/bin/infection --only-covered --show-mutations"
    }
}
